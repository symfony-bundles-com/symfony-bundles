{
    "name": "symfony-bundles-com/symfony-bundles",
    "description" : "Search engine for Symfony Bundles",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/symfony-bundles-com/symfony-bundles",
    "authors" : [
      {
        "name" : "Alexey Samara",
        "role": "Lead",
        "email" : "lion.samara@gmail.com",
        "homepage": "https://wow-apps.pro"
      },
      {
        "name" : "Alexander Erin",
        "role": "Developer",
        "email" : "larrya65@gmail.com",
        "homepage": "https://github.com/larry27"
      }
    ],
    "support": {
      "issues": "https://github.com/symfony-bundles-com/symfony-bundles/issues",
      "wiki": "https://github.com/symfony-bundles-com/symfony-bundles/wiki",
      "source": "https://github.com/symfony-bundles-com/symfony-bundles"
    },
    "keywords" : [
      "symfony",
      "bundles",
      "search"
    ],
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/web-server-bundle": "^4.0",
        "symfony/yaml": "^4.0",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "guzzlehttp/guzzle": "^6.3",
        "wow-apps/symfony-packagist": "^1"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "phpunit/phpunit": "^6.5",
        "symfony/phpunit-bridge": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1DA7G2W2G0Y5H9X8N9JCK5P",
            "allow-contrib": false
        }
    }
}
